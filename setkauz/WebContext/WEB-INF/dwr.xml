<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN"
    "http://getahead.org/dwr/dwr20.dtd">

<dwr>
	<allow>
		<create creator="new" javascript="Session">
			<param name="class" value="org.dwr.Session" />
		</create>

		<create creator="new" javascript="LocalUserProxy">
			<param name="class" value="org.supposition.db.proxy.LocalUserProxy" />
		</create>

		<create creator="new" javascript="UserProxy">
			<param name="class" value="org.supposition.db.proxy.UserProxy" />
		</create>
		
		<create creator="new" javascript="RoleProxy">
			<param name="class" value="org.supposition.db.proxy.RoleProxy" />
		</create>
		
		<create creator="new" javascript="SystemDefaultsProxy">
			<param name="class" value="org.supposition.proxy.SystemDefaultsProxy" />
		</create>
		
		<convert match="org.supposition.db.proxy.RoleBean" converter="bean"/>
		<convert match="org.supposition.db.proxy.UserBean" converter="bean"/>
		<convert match="org.supposition.db.proxy.KeyValueBean" converter="bean"/>
	</allow>	

</dwr>
