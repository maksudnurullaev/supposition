<?xml version="1.0" encoding="utf-8"?>
<data-map project-version="2.0">
	<property name="defaultPackage" value="org.supposition.db"/>
	<db-entity name="Roles">
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="Name" type="VARCHAR" isMandatory="true" length="32"/>
	</db-entity>
	<db-entity name="Users">
		<db-attribute name="Additionals" type="VARCHAR" length="256"/>
		<db-attribute name="Created" type="VARCHAR" length="23"/>
		<db-attribute name="ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="Mail" type="VARCHAR" isMandatory="true" length="50"/>
		<db-attribute name="Password" type="VARCHAR" isMandatory="true" length="64"/>
		<db-attribute name="Salt" type="VARCHAR" length="128"/>
		<db-attribute name="Status" type="VARCHAR" length="16"/>
		<db-attribute name="Updated" type="VARCHAR" length="23"/>
		<db-attribute name="VerifyString" type="VARCHAR" length="64"/>
	</db-entity>
	<db-entity name="Users2Roles">
		<db-attribute name="RoleID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="UserID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
	</db-entity>
	<obj-entity name="Role" className="org.supposition.db.Role" dbEntityName="Roles">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="Name"/>
	</obj-entity>
	<obj-entity name="User" className="org.supposition.db.User" dbEntityName="Users">
		<obj-attribute name="additionals" type="java.lang.String" db-attribute-path="Additionals"/>
		<obj-attribute name="created" type="java.lang.String" db-attribute-path="Created"/>
		<obj-attribute name="mail" type="java.lang.String" db-attribute-path="Mail"/>
		<obj-attribute name="password" type="java.lang.String" db-attribute-path="Password"/>
		<obj-attribute name="salt" type="java.lang.String" db-attribute-path="Salt"/>
		<obj-attribute name="status" type="java.lang.String" db-attribute-path="Status"/>
		<obj-attribute name="updated" type="java.lang.String" db-attribute-path="Updated"/>
		<obj-attribute name="verifyString" type="java.lang.String" db-attribute-path="VerifyString"/>
	</obj-entity>
	<db-relationship name="RoleIDs" source="Roles" target="Users2Roles" toMany="true">
		<db-attribute-pair source="ID" target="RoleID"/>
	</db-relationship>
	<db-relationship name="UserIDs" source="Users" target="Users2Roles" toMany="true">
		<db-attribute-pair source="ID" target="UserID"/>
	</db-relationship>
	<db-relationship name="Roles" source="Users2Roles" target="Roles" toDependentPK="true" toMany="false">
		<db-attribute-pair source="RoleID" target="ID"/>
	</db-relationship>
	<db-relationship name="Users" source="Users2Roles" target="Users" toDependentPK="true" toMany="false">
		<db-attribute-pair source="UserID" target="ID"/>
	</db-relationship>
	<obj-relationship name="Users" source="Role" target="User" deleteRule="Nullify" db-relationship-path="RoleIDs.Users"/>
	<obj-relationship name="Roles" source="User" target="Role" deleteRule="Nullify" db-relationship-path="UserIDs.Roles"/>
</data-map>
